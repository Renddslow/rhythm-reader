[{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse","reference":{"id":"PS1:1","chapter":1,"book":"Ps","verse":1,"readable":"Psalms 1:1"}},{"type":"char","content":"Blessed is the man who does not walk in the counsel of the wicked,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Nor stand in the path of sinners,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Nor sit in the seat of scoffers!"}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS1:1","chapter":1,"book":"Ps","verse":1,"readable":"Psalms 1:1"}},{"type":"verse","reference":{"id":"PS1:2","chapter":1,"book":"Ps","verse":2,"readable":"Psalms 1:2"}},{"type":"char","content":"But his delight is in the Law of the Lord,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And on His Law he meditates day and night."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS1:2","chapter":1,"book":"Ps","verse":2,"readable":"Psalms 1:2"}},{"type":"verse","reference":{"id":"PS1:3","chapter":1,"book":"Ps","verse":3,"readable":"Psalms 1:3"}},{"type":"char","content":"He will be like a tree planted by streams of water,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Which yields its fruit in its season,"}]},{"type":"poetry_line","indent":1,"children":[{"type":"char","content":"And its leaf does not wither;"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"And in whatever he does, he prospers."}]}]},{"type":"poetry","children":[{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS1:3","chapter":1,"book":"Ps","verse":3,"readable":"Psalms 1:3"}},{"type":"verse","reference":{"id":"PS1:4","chapter":1,"book":"Ps","verse":4,"readable":"Psalms 1:4"}},{"type":"char","content":"The wicked are not so,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"But they are like chaff which the wind blows away."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS1:4","chapter":1,"book":"Ps","verse":4,"readable":"Psalms 1:4"}},{"type":"verse","reference":{"id":"PS1:5","chapter":1,"book":"Ps","verse":5,"readable":"Psalms 1:5"}},{"type":"char","content":"Therefore the wicked will not stand in the judgment,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"Nor sinners in the assembly of the righteous."}]},{"type":"poetry_line","indent":1,"children":[{"type":"verse_end","reference":{"id":"PS1:5","chapter":1,"book":"Ps","verse":5,"readable":"Psalms 1:5"}},{"type":"verse","reference":{"id":"PS1:6","chapter":1,"book":"Ps","verse":6,"readable":"Psalms 1:6"}},{"type":"char","content":"For the Lord knows the way of the righteous,"}]},{"type":"poetry_line","indent":2,"children":[{"type":"char","content":"But the way of the wicked will perish."}]},{"type":"verse_end","reference":{"id":"PS1:6","chapter":1,"book":"Ps","verse":6,"readable":"Psalms 1:6"}}]}]